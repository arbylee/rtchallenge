<%= form_for @challenge do |f| %>
  <%= f.select :user_ids, options_from_collection_for_select(@users, :id, :email), {}, { multiple: true } %>
  <br />
  <%= f.select :movie_ids, options_from_collection_for_select(@movies, :id, :title), {}, { multiple: true } %>
  <br />
  <%= f.submit %>
<% end %>
